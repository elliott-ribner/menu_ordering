<div class="content-block">
  <h1> create menu </h1>
  <%= render 'form' %>
  <div>
    <h3>Import a CSV File</h3>
    <%= form_tag import_shop_items_path(@shop), multipart: true do %>
    <%= file_field_tag :file %>
    <%= submit_tag "Import CSV" %>
    <% end %>
  </div>
  <%= link_to 'view menu', shop_items_path(@shop) %>
</div>
